		for (int num1 : array) {
			System.out.print(num1 + " ");
		}
	}
	public static void shuffleArray(int[] array) {
		int n = array.length;
		for (int i = n - 1; i > 0; i--) {
			int j = (int) (Math.random() * (i + 1));
			swap(array, i, j);
		}

	}
	public static void swap(int[] array, int i, int j) {
		int temp = array[i];
		array[i] = array[j];
		array[j] = temp;
	}

}
